{
	"version": "2.0.0",
	"tasks": [
    {
      "type": "process",
      "label": "C/C++: g++.exe build active file",
      "command": "g++",
      "args": [
        "--std=c++17",
        "${file}",
        "-I${workspaceFolder}/boost_1_81_0",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}2.exe"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
    }
  ]
}